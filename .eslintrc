{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:flowtype/recommended"],
  "plugins": ["react", "jsx-a11y", "flowtype"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-underscore-dangle": 0,
    "react/forbid-prop-types": [0, { "forbid": ["object"] }]
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  }
}
